{
  "name": "team_7",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "build:styles": "tailwindcss build styles/globals.css -o public/styles.css"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "next": "14.0.3",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.32",
    "react": "^18",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
